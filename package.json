    "watch": "babel src --out-dir dist --watch"
  },
  "keywords": [
    "ardoq",
    "sdk",
    "api"
  ],
  "author": "Adrian Albrektsen",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "json2csv": "^6.0.0-alpha.2",
    "prompt-sync": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "eslint": "^9.24.0",
    "jest": "^29.7.0"
  }
}
