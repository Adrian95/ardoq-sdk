{
  "name": "ardoq-sdk",
  "version": "1.0.0",
  "description": "A JavaScript SDK for interacting with the Ardoq API.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "ardoq-cli": "src/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --passWithNoTests",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.js",
